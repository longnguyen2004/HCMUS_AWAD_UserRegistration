{
    "$schema": "https://openapi.vercel.sh/vercel.json",
	"functions": {
		"app/backend/dist/*.js": {
			"memory": 512
		}
	},
    "rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "app/backend/dist"
		},
		{
			"source": "/assets/:path*",
			"destination": "app/frontend/dist/assets/:path*"
		},
		{
			"source": "/(.*)",
			"destination": "app/frontend/dist/index.html"
		}
    ]
}
